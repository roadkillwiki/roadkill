  <data name="Page3_Label1" xml:space="preserve">
    <value>Chcete uživatele ověřovat v Active Directory?</value>
  </data>
  <data name="Page3_Group1_Title" xml:space="preserve">
    <value>Ověřování Windows</value>
  </data>
  <data name="Page3_Group1_Label" xml:space="preserve">
    <value>Používat Windows ověřování</value>
  </data>
